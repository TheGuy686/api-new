{
	"name": "Eezze",
	"version": "1.0.0",
	"description": "",
	"main": "dist/src/index.js",
	"scripts": {
		"prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
		"postbuild": "copyfiles src/**/*.ezt src/**/*.ezt dist/",
		"build": "tsc",
		"prestart": "npm run build",
		"start": "node .",
		"watch-till-kill": "tsc-watch --onFirstSuccess 'node ./dist/src/index.js'",
		"test": "SERVER_PORT=3001 ./node_modules/.bin/mocha --reporter ./plugins/eezze/base/unit-test/EezzeReporter.js ./plugins/eezze/unit-test",
		"run-migrations": "node ./src/migrations/index.js",
		"create-database": "node ./src/migrations/create-database.js",
		"wgen2": "tsc-watch --onSuccess 'ts-node ./src/servers/generation-server.ts'",
		"w-exec-test": "tsc-watch --onSuccess 'ts-node ./src/dev/unit-test/do-requet.ts'",
		"w-exec-ws": "tsc-watch --onSuccess 'ts-node ./src/dev/development/ws-controller/index.ts'",
		"w-exec-ws-rec": "tsc-watch --onSuccess 'ts-node ./src/dev/development/ws-controller/reconnecting-client.ts'",
		"ck-port": "tsc-watch --onSuccess 'ts-node ./dev/check-port.ts'",
		"exec-ws-rec": "ts-node ./src/dev/development/ws-controller/reconnecting-client.ts",
		"wgen-till-kill": "tsc-watch --onFirstSuccess 'ts-node ./src/servers/generation-server.ts'",
		"start-logging-server": "node ./plugins/eezze/servers/logging-server.js",
		"wlogging-till-kill": "node ./plugins/eezze/servers/logging-server.js",
		"watch-request-bodies-dev": "nodemon --watch './**/*' -e ts,tsx --exec 'ts-node ./plugins/eezze-generator/dev/independant-dev/request-bodies.ts'",
		"watch": "tsc-watch --onSuccess 'ts-node --transpile-only ./src/apps/eezze-project-api.ts'",
		"watch2": "ts-node ./src/servers/eezze-service-api.ts",
		"wgen": "tsc-watch --onSuccess 'ts-node --transpile-only ./src/apps/generation-ws-server.ts'",
		"wlogger": "tsc-watch --onSuccess 'ts-node --transpile-only ./src/apps/generation-ws-log-server.ts'",
		"wstats": "tsc-watch --onSuccess 'ts-node --transpile-only ./src/apps/eezze-ws-stats.ts'",
		"wtrans": "tsc-watch --onSuccess 'ts-node --transpile-only ./src/servers/eezze-ws-translations.ts'",
		"ut-business-logic": "clear; nodemon -e ts,js,ezt --ignore 'dev/business-logic-uts/output/**' --ignore 'dist/**' --exec 'ts-node ./dev/business-logic-uts/index.ts'",
		"ut-bl-dec-api": "clear; sleep 1; nodemon -e ts,js,ezt --ignore 'dev/business-logic-uts/output/**' --ignore 'dist/**' --exec 'ts-node ./dev/business-logic-uts/run-tu-requests.ts'",
		"ut-bl-prop": "clear; nodemon -e ts,js,ezt --ignore 'dev/business-logic-uts/output/**' --ignore 'dist/**' --exec 'ts-node ./dev/business-logic-uts/parse-prop.ts'",
		"ut-bl-prop-obj": "clear; nodemon -e ts,js,ezt --ignore 'dev/business-logic-uts/output/**' --ignore 'dist/**' --exec 'ts-node ./dev/business-logic-uts/parse-prop-obj.ts'",
		"ut-login": "clear; nodemon -e ts,js,ezt --ignore 'dev/business-logic-uts/output/**' --ignore 'dist/**' --exec 'ts-node ./dev/ut-login.ts'",
		"ut-exec-prop": "clear; nodemon -e ts,js,ezt --ignore 'dev/business-logic-uts/output/**' --ignore 'dist/**' --exec 'ts-node ./dev/business-logic-uts/exec.ts'",
		"ut-fe-cm": "clear; nodemon -e ts,js,ezt --ignore 'dist/**' --exec 'cp dev/fe-context-menu/Actions.ts ../web/classes/menu/; cp dev/fe-context-menu/LogicChain.ts ../web/classes/menu/; cp dev/fe-context-menu/MenuState.ts ../web/classes/menu/; cp dev/fe-context-menu/StateLogger.ts ../web/classes/menu/; ts-node ./dev/fe-context-menu/index.ts'",
		"ai-nlp-main": "clear; nodemon -e ts,js,ezt --ignore 'dist/**' --exec 'ts-node ./dev/ai/index.ts'",
		"api-logs": "docker logs --tail 1000 -f eezze-rest-1",
		"ws-gen-logs": "docker logs --tail 1000 -f eezze-ws-1",
		"ws-stats-logs": "docker logs --tail 1000 -f eezze-ws-stats-1",
		"ws-logger-logs": "docker logs --tail 1000 -f eezze-ws-logger-1",
		"sys-stats": "ts-node plugins/eezze/services/get-system-info.ts --prefix /home/theguy/docker/api",
		"test-logs": "nodemon -e ts,js,ezt --ignore 'dist/**' --exec 'node ./dev/test-logs.js'",
		"con-to-logging-server": "nodemon --exec 'clear; node ./dev/connecting-to-logging-ws-test.ts'",
		"update-local-dev-project": "rm -rf /var/www/eezze/projects/115/plugins/eezze/; rm -rf /var/www/eezze/projects/115/dist; cp -R ~/docker/api/plugins/eezze/ /var/www/eezze/projects/115/plugins",
		"w-ut-dev": "nodemon -e ts,js,ezt --ignore 'dist/**' --exec 'cp ~/docker/api/plugins/eezze/unit-test/* /var/www/eezze/projects/115/plugins/eezze/unit-test; sleep 1; npm run --prefix /var/www/eezze/projects/115 run-unit-tests uts=DemoService.Rest.aTest.default'",
		"run-unit-tests": "node ./plugins/eezze/unit-test/index.js host=$HOST_IP port=3100 prId=115",
		"w-run-unit-tests": "nodemon -e ts,js,ezt --ignore 'dist/**' --exec 'node ./plugins/eezze/unit-test/index.js host=192.168.47.192 port=3100 prId=115 uts=EezzeServices.Rest.TestRest.default'",
		"generate-docs": "tsc-watch --onSuccess 'ts-node --transpile-only ./plugins/eezze/services/output-docs.ts'"
	},
	"author": "Ryan J. Cooke",
	"license": "ISC",
